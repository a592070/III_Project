    <bean id="highSpeedRailDao" class="innocence741.model.HighSpeedRailDAO" lazy-init="true" scope="session"/>
    <bean id="t_Order_ListDAO" class="innocence741.model.T_Order_ListDAO" lazy-init="true" scope="session"/>
    
    <bean id="carRentalCompanyService" class="innocence741.service.CarRentalCompanyService" lazy-init="true" scope="session"/>
